<%- include('header'); -%>
    <div class="content">
    <div class="day">
      <h1><%= listTitle %></h1>
    </div>
    <div class="boxes">
      <ul>
        <%for(let data of newListItem){%>
            <li><input type="checkbox"><p><%= data.name %></p></li>
        <%}%>
      </ul>
      <form action="/" method="POST">
        <input type="text" name="newItem" placeholder="New item" autocomplete="off"/>
        <button type="submit" name="list" value="<%= listTitle %>"><a>+</a></button>
      </form>
    </div>
  </div>
  <%- include('footer'); -%>